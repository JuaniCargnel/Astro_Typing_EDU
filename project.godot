config_version=4

[application]

config/name="Astro Typing"
config/description="A 2D game in which you have to help a little alien who must destroy and dodge ships by typing the words that appear below them."
run/main_scene="res://Scenes/Main.tscn"
config/icon="res://Logo_UNL.png"

[autoload]

PromptList="*res://Scripts/PromptList.gd"
DefinitionList="*res://Scripts/DefinitionList.gd"
Valores="*res://Scripts/Valores.gd"

[display]

window/size/width=1920
window/size/height=1080
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
mouse_cursor/custom_image="res://Assets/Extra/Select_purple.png"
window/stretch/mode="viewport"

